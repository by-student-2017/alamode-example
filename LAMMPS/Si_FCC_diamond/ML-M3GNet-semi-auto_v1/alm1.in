&general
  PREFIX = sc222_harm
  MODE = optimize
  NAT = 64; NKD = 1
  KD = Si 
/

&optimize
  DFSET = displace/DFSET_harmonic
/
 
&interaction
  NORDER = 1  # 1: harmonic, 2: cubic, ..
/

