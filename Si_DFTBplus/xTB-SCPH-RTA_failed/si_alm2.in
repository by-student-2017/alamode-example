&general
  PREFIX = si222_anharm
  MODE = optimize
  NAT = 64; NKD = 1
  KD = Si
/

&optimize
 DFSET = displace/DFSET_random
 FC2XML = si222_harm.xml
 LMODEL = enet
 NDATA = 20
 CV = 4
 L1_RATIO = 1.0
 CONV_TOL = 1.0e-8
/

&interaction
  NORDER = 3  # 1: harmonic, 2: cubic, ..
  NBODY = 2 3 3
/

&cell
  20.406 # factor in Bohr unit
  1.0 0.0 0.0 # a1
  0.0 1.0 0.0 # a2
  0.0 0.0 1.0 # a3
/

&cutoff 
  Si-Si 8.1 8.1 8.1
/


